            <http://localhost/pipeline/b67542e2> ,
            <http://localhost/pipeline/c67542e2> ;
		skos:prefLabel "Disabled TextHolder and FilesToRdf" ;
        lp:profile <http://localhost/execution-debug/profile> ;
